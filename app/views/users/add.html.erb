<h1>Add new user</h1>
<%= form_tag create_user_path do %>
<p> First Name <br/><%= text_field_tag 'first_name' %></p>
<p> Last Name <br/><%= text_field_tag 'last_name' %></p>
<p> Email <br/><%= text_field_tag 'email' %></p>
<p> Gender <br/><%= select_tag 'gender', options_for_select(@gender_options) %></p>
<p> Bio <br/><%= text_area_tag 'bio' %></p>
<p> <%= submit_tag 'Create' %>
<% end %>

